type <T> list = Empty | Cons(T, list @ T)

fun sum_list (l : list @ int) : int {
    match (l) {
        Empty {return 0},
        Cons (var i, var rest) {return i + sum_list (rest)}
    }
}

fun main () : void {
    let banana : list @ int = Empty @ int;
    let banana2 := Cons @ int (8, banana);
    let string_banana : list @ char = Empty @ char;
    return;
}
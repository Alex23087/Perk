import "stdio.h"

type Example =
	| Something ((char*)?)

fun print_example(e: Example): void {
	match (e) {
		Something (var s) {
			if (s?) {
				printf("Got a string: %s\n", s!);
			} else {
				printf("Got nothing\n");
			}
		}
	}
}

fun main(): void {
	let e1: Example = Something(some "hi");
	let e2: Example = Something(none);

	print_example(e1);
	print_example(e2);
}
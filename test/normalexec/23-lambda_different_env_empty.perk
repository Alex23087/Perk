import "stdio.h"

extern printf: (char*, ...) -> void

fun main (): int {
    let x: int = 0;
    let y: int = 0;

    let f := (): void {
        printf("%d\n", x);
    };

    let g := (): void {
        printf("%d\n", y);
    };

    f = g;

    f = (): void {
        printf("%d %d\n", 32, 32);
    };

    f();
}
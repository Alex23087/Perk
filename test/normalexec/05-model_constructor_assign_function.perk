import "stdio.h"
import "malloc.h"

extern printf: (char*, ...) -> void

fun f (): void {
    printf("f()\n");
}

model Test {
    let constructor: () -> void = f,
    let f: () -> void = f,

    fun constructor(): void {
        printf("Test.constructor()\n");
    },

    let callback: () -> void = (): void => {return;},

    let doSomething: () -> void = (): void => {
        printf("doSomething()\n");
        self.callback();
    }
}

fun main (): int {
    let x := summon Test();
    // x.callback = ???;

    let fu: () -> void = f;
    fu();
    return 0;
}
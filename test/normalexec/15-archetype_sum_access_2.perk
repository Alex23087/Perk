extern printf: (char*, ...) -> void

archetype StringContainer {
    str: char*
}

model Test: StringContainer {
    let str: char* = "Hello, World!"
}

fun main(): void {
    let test := summon Test();
    let ctr := (test as StringContainer);
    ctr.str = "Hello, Universe!";
    printf("%s", test.str);
    printf("%s", ctr.str);
    banish test;
}